{
  "name": "pos-helper",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "engines": {
    "node": ">=4"
  },
  "build": {
    "appId": "demo.kather.co",
    "productName": "Event Manager Powered by PeopleAppz",
    "asar": true,
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ],
      "icon": "assets/a.ico"
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "shortcutName": "peopleappz",
      "oneClick": false,
      "installerIcon": "assets/a.ico",
      "uninstallerIcon": "assets/a.ico",
      "artifactName": "peopleappz.exe",
      "license": "LICENSE",
      "runAfterFinish": true
    }
  },
  "scripts": {
    "start": "electron .",
    "pack": "build"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "express": "^4.17.1"
  },
  "devDependencies": {
    "electron": "^11.5.0",
    "electron-builder": "^21.2.0"
  },
  "author": "",
  "license": "ISC"
}
